<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"		android:gravity="center"
    android:orientation="vertical" 	android:background="@drawable/bkg_img_default1">
	
    <com.gdpost.aming.MyImageView
            android:id="@+id/yc_cmd"
            android:layout_width="wrap_content"	 android:scaleType="matrix"
            android:layout_height="wrap_content"	android:layout_margin="2dp"	
            android:adjustViewBounds="true"
            android:src="@drawable/yc" />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical" >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >
                <com.gdpost.aming.MyImageView
                    android:id="@+id/tb_cmd"
                    android:layout_width="wrap_content"	 android:scaleType="matrix"
                    android:layout_height="wrap_content"	android:layout_margin="2dp"	
                    android:adjustViewBounds="true"
                    android:src="@drawable/tb" />
               <com.gdpost.aming.MyImageView
                    android:id="@+id/jsbe_cmd"
                    android:layout_width="wrap_content"	 android:scaleType="matrix"
                    android:layout_height="wrap_content"	android:layout_margin="2dp"	
                    android:adjustViewBounds="true"
                    android:src="@drawable/jsbe" />
            </LinearLayout>
            <com.gdpost.aming.MyImageView
                 android:id="@+id/zyjk_cmd"
                android:layout_width="wrap_content"	 android:scaleType="matrix"
                android:layout_height="wrap_content"	android:layout_margin="2dp"	
                android:adjustViewBounds="true"
                android:src="@drawable/zyjk" />
            <com.gdpost.aming.MyImageView
                 android:id="@+id/zyhk_cmd"
                android:layout_width="wrap_content"	 android:scaleType="matrix"
                android:layout_height="wrap_content"	android:layout_margin="2dp"	
                android:adjustViewBounds="true"
                android:src="@drawable/zyhk" />
        </LinearLayout>
       <com.gdpost.aming.MyImageView
            android:id="@+id/bdbf_cmd"
            android:layout_width="wrap_content"	 android:scaleType="matrix"
            android:layout_height="wrap_content"	android:layout_margin="2dp"	
            android:adjustViewBounds="true"
            android:src="@drawable/bdbf" />
       

       <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >
            <com.gdpost.aming.MyImageView
                 android:id="@+id/scjlq_cmd"
                android:layout_width="wrap_content"	 android:scaleType="matrix"
                android:layout_height="wrap_content"	android:layout_margin="2dp"	
                android:adjustViewBounds="true"
                android:src="@drawable/scjlq" />
            <com.gdpost.aming.MyImageView
                 android:id="@+id/jfxxbg_cmd"
                android:layout_width="wrap_content"	 android:scaleType="matrix"
                android:layout_height="wrap_content"	android:layout_margin="2dp"	
                android:adjustViewBounds="true"
                android:src="@drawable/jfxxbg" />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >
                <com.gdpost.aming.MyImageView
                    android:id="@+id/tbrbg_cmd"
                    android:layout_width="wrap_content"	 android:scaleType="matrix"
                    android:layout_height="wrap_content"	android:layout_margin="2dp"	
                    android:adjustViewBounds="true"
                    android:src="@drawable/tbrbg" />
               <com.gdpost.aming.MyImageView
                    android:id="@+id/syrbg_cmd"
                    android:layout_width="wrap_content"	 android:scaleType="matrix"
                    android:layout_height="wrap_content"	android:layout_margin="2dp"	
                    android:adjustViewBounds="true"
                    android:src="@drawable/syrbg" />
            </LinearLayout>
            
        </LinearLayout>
       <com.gdpost.aming.MyImageView
            android:id="@+id/search_cmd"
            android:layout_width="wrap_content"	 android:scaleType="matrix"
            android:layout_height="wrap_content"	android:layout_margin="2dp"	
            android:adjustViewBounds="true"
            android:src="@drawable/search" />
    </LinearLayout>

</LinearLayout>